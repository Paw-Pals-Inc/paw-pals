  "bugs": {
    "url": "https://github.com/Paw-Pals-Inc/paw-pals/issues"
  },
  "homepage": "https://github.com/Paw-Pals-Inc/paw-pals#readme",
  "devDependencies": {
    "concurrently": "^8.0.1"
  }
}
