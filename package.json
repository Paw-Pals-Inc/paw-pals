{
  "name": "paw-pals",
  "version": "1.0.0",
  "description": "Web app that allows dog owners to set up playdates for their dogs <br>",
  "scripts": {
    "start:frontend": "cd frontend && npm start",
    "start:backend": "cd backend && npm start",
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "install:frontend": "cd frontend && npm install",
    "install:backend": "cd backend && npm install",
    "install-all": "concurrently \"npm run install:frontend\" \"npm run install:backend\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Paw-Pals-Inc/paw-pals.git"
  },
  "keywords": [],
